<p id="notice"><%= notice %></p>
<div class="container">
  <div class="col-sm-12">
    <div class="bs-calltoaction bs-calltoaction-default">
      <div class="row">
        <div class="col-md-9 cta-contents">
          <div class="panel-heading">          
            <table class="table table-user-information">
              <thead>
                <tr>      
                  <th>Nombre</th>
                  <th>Telefono</th>
                  <th>Correo</th>
                  <th>Direccion</th>
                  <th>Empresa</th>
                  <th colspan="3"></th>
                </tr>
              </thead>
              <tbody>
                <% @predios.each do |predio| %>
                <tr>
                  <td class="col-sm-2"><%= predio.Nombre %></td>
                  <td class="col-sm-2"><%= predio.Telefono %></td>
                  <td class="col-sm-2"><%= predio.Correo %></td>
                  <td class="col-sm-2"><%= predio.Direccion %></td>
                  <td class="col-sm-2"><%= Company.find(predio.companies_id).Nombre %></td>
                  <td><%= link_to 'Mostrar', predio, class: "btn btn-default btn-x" %></td>
                  <td><%= link_to 'Editar', edit_predio_path(predio), class: "btn btn-default btn-x" %></td>
                  <td><%= link_to "Eliminar", predio, method: :delete, data: {confirm: "Esta Seguro ?"}, class: "btn btn-default btn-x" %></td>
                </tr>
                <% end %>
              </tbody>
            </table>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>